{
    "name": "TodoApp",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "android:run": "clear && npm run android:clean && react-native run-android",
        "android:clean": "clear && cd android/ && ./gradlew clean && cd ..",
        "android:production": "react-native run-android --variant release",
        "android:compile": "clear && cd android && ./gradlew assembleRelease && cd ..",
        "android:bundle": "clear && cd android && ./gradlew bundleRelease && cd ..",
        "ios:run": "clear && ",
        "ios:reset-pods": "clear && cd ios; rm -rf ~/Library/Caches/CocoaPods; rm -rf Pods; rm -rf ~/Library/Developer/Xcode/DerivedData/*; pod deintegrate; pod setup; pod install; cd ..",
        "test": "jest",
        "lint": "clear && eslint ."
    },
    "dependencies": {
        "@react-native-community/async-storage": "1.9.0",
        "@react-native-community/masked-view": "0.1.9",
