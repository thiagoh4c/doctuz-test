        "@react-navigation/material-top-tabs": "5.1.15",
        "@react-navigation/native": "5.2.1",
        "@react-navigation/stack": "5.2.16",
        "axios": "0.19.2",
        "date-fns": "2.12.0",
        "firebase": "^8.3.2",
        "flatted": "2.0.2",
        "formik": "2.1.4",
        "qs": "6.9.4",
        "react": "16.11.0",
