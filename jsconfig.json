{
    "compilerOptions": {
        "baseUrl": "./",
        "paths": {
            "@components/*": [
                "src/components/*"
            ],
            "@store/*": [
                "src/store/ducks/*"
            ],
            "@services/*": [
                "src/services/*"
            ],
            "@styles/*": [
                "src/assets/styles/*"
            ],
            "@img/*": [
                "src/assets/img/*"
            ],
            "@svg/*": [
                "src/assets/svg/*"
            ],
            "@src/*": [
                "src/*"
            ],
            "@pages/*": [
                "src/pages/*"
            ],
        }
    },
    "exclude": [
        "node_modules"
    ]
}